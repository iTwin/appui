{
  "name": "test-app",
  "type": "module",
  "description": "AppUI Test Application",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "http://localhost:3000/",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run package:cjs && tsc -p tsconfig.backend.json",
    "build:frontend": "npm run copy:webfont && npm run pseudolocalize && vite build",
    "package:cjs": "node ./scripts/package-cjs.js ./lib",
    "preview": "vite preview --port 3000",
    "clean": "rimraf -g lib build dist",
    "electron": "run-p start:webserver start:electron",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "start": "run-p start:webserver start:backend",
    "start:electron": "cross-env NODE_ENV=development electron ./lib/backend/main.js",
    "start:electron:prod": "electron ./lib/backend/main.js",
    "start:webserver": "npm run copy:webfont && npm run pseudolocalize && vite",
    "start:backend": "node --max-http-header-size=16000 lib/backend/main.js",
    "pseudolocalize": "npm run copy:locales && betools pseudolocalize --englishDir ./lib/locales/en --out ./lib/locales/en-PSEUDO",
    "docs:extract": "betools extract --fileExt=ts,tsx --extractFrom=./src --recursive --out=../../../generated-docs/extract",
    "docs": "npm run docs:extract",
    "copy:locales": "copyfiles -f ./node_modules/@itwin/*/lib/public/locales/en/*.json ./public/locales/en/*.json ./lib/locales/en",
    "copy:webfont": "copyfiles -f ./node_modules/@bentley/icons-generic-webfont/dist/**/* ./lib/webfont"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
