    "cpx2": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.11.1",
    "inversify": "^6.0.1",
    "prop-types": "^15.8.1",
    "reflect-metadata": "^0.1.13",
    "storybook": "^8.3.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "~8.11.0",
