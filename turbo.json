{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "lib/**", "build/**"],
        "outputMode": "new-only"
      },
      "build:ci": {
        "dependsOn": ["^build:ci"],
        "outputs": ["dist/**", "lib/**", "build/**"],
        "outputMode": "new-only"
      }
    }
}
