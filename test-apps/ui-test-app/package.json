{
  "name": "ui-test-app",
  "description": "UI Test Application",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "http://localhost:3000/",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run copy:assets && tsc -p tsconfig.backend.json",
    "build:frontend": "npm run pseudolocalize && cross-env GENERATE_SOURCEMAP=false react-scripts build --unsafe-fast-sass",
    "clean": "rimraf lib build .rush package-deps.json",
    "copy:assets": "cpx ./src/backend/web/BackendServer.config.json ./lib/backend/web",
    "lint": "tslint -p . 1>&2",
    "start": "run-p start:webserver start:electron",
    "start:electron": "electron ./lib/backend/main.js",
    "start:webserver": "npm run pseudolocalize && cross-env BROWSER=none react-scripts start --unsafe-fast-sass",
    "start:backend": "node --max-http-header-size=16000 lib/backend/main.js",
    "start:extensionserver": "node ./node_modules/@bentley/imodeljs-webserver/lib/WebServer.js --port=4200 --resources=./lib/extensionserver/",
    "start:servers": "run-p \"start:webserver\" \"start:backend\"",
    "start:threeservers": "run-p \"start:webserver\" \"start:backend\" \"start:extensionserver\"",
    "pseudolocalize": "node ./node_modules/@bentley/build-tools/scripts/pseudolocalize.js --englishDir ./public/locales/en --out ./build/public/locales/en-PSEUDO",
    "test": "",
    "cover": "",
    "docs": ""
  },
  "repository": {},
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
