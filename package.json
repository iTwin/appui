{
  "name": "appui",
  "//private": "NOTE: This 'root' package is private. It is never published",
  "private": true,
  "description": "iTwin.js appui components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/appui"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:lage": "lage build",
    "build:clean": "lage build --no-cache",
    "build:profile": "lage build --no-cache --profile",
    "clean": "lage clean --no-cache && cospace purge && pnpm i -w cospace",
    "setOverrides": "cospace override",
    "purge": "cospace purge",
    "build:turbo": "turbo run build",
    "change": "beachball"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "digital-twin",
    "iTwin"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "//dependencies": "NOTE: dependencies will be empty here and instead specified in each package.",
  "//devDependencies": "NOTE: devDependencies will be empty here and instead specified in each package.",
  "devDependencies": {
    "beachball": "^2.31.4",
    "cospace": "latest",
    "lage": "^1.9.2",
    "syncpack": "^8.2.4",
    "turbo": "^1.5.6"
  },
  "pnpm": {
    "overrides": {
      "@itwin/appui-abstract": "workspace:*",
      "@itwin/appui-layout-react": "workspace:*",
      "@itwin/appui-react": "workspace:*",
      "@itwin/appui-test-providers": "workspace:*",
      "@itwin/components-react": "workspace:*",
      "@itwin/core-react": "workspace:*",
      "@itwin/imodel-components-react": "workspace:*",
      "@itwin/ui-items-providers-test": "workspace:*",
      "internal-tools": "workspace:*",
      "@bentley/imodeljs-native": "3.3.15",
      "@itwin/backend-webpack-tools": "3.3.5",
      "@itwin/build-tools": "3.3.5",
      "@itwin/certa": "3.3.5",
      "@itwin/core-backend": "3.3.5",
      "@itwin/core-bentley": "3.3.5",
      "@itwin/core-common": "3.3.5",
      "@itwin/core-electron": "3.3.5",
      "@itwin/core-extension": "3.3.5",
      "@itwin/core-frontend": "3.3.5",
      "@itwin/core-geometry": "3.3.5",
      "@itwin/core-i18n": "3.3.5",
      "@itwin/core-markup": "3.3.5",
      "@itwin/core-mobile": "3.3.5",
      "@itwin/core-orbitgt": "3.3.5",
      "@itwin/core-quantity": "3.3.5",
      "@itwin/core-telemetry": "3.3.5",
      "@itwin/core-transformer": "3.3.5",
      "@itwin/core-webpack-tools": "3.3.5",
      "@itwin/ecschema-editing": "3.3.5",
      "@itwin/ecschema-locaters": "3.3.5",
      "@itwin/ecschema-metadata": "3.3.5",
      "@itwin/ecschema-rpcinterface-common": "3.3.5",
      "@itwin/ecschema-rpcinterface-impl": "3.3.5",
      "@itwin/ecschema2ts": "3.3.5",
      "@itwin/eslint-plugin": "3.3.5",
      "@itwin/express-server": "3.3.5",
      "@itwin/frontend-devtools": "3.3.5",
      "@itwin/hypermodeling-frontend": "3.3.5",
      "@itwin/perf-tools": "3.3.5",
      "@itwin/presentation-backend": "3.3.5",
      "@itwin/presentation-common": "3.3.5",
      "@itwin/presentation-components": "3.3.5",
      "@itwin/presentation-frontend": "3.3.5",
      "@itwin/presentation-opentelemetry": "3.3.5",
      "@itwin/presentation-testing": "3.3.5",
      "@itwin/webgl-compatibility": "3.3.5"
    },
    "allowNonAppliedPatches": true,
    "patchedDependencies": {},
    "peerDependencyRules": {
      "allowAny": [
        "@babel/*",
        "eslint",
        "typescript"
      ],
      "ignoreMissing": [
        "@babel/*",
        "@eslint/*",
        "@testing-library/*",
        "@typescript-eslint/*"
      ]
    }
  }
}
