{
  "name": "@bentley/ecjs",
  "version": "0.0.1",
  "description": "ECObjects core concepts in typescript",
  "license": "UNLICENSED",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://tfs.bentley.com/tfs/ProductLine/Platform%20Technology/_git/ec-js"
  },
  "scripts": {
    "clean": "rimraf lib .nyc_output",
    "lint": "node ./node_modules/@bentley/bentleyjs-tools/scripts/tslint.js",
    "prebuild": "npm run lint & npm run clean",
    "build": "tsc",
    "test": "node ./node_modules/@bentley/bentleyjs-tools/scripts/test-tsnode.js --testDir=./test/",
    "test:watch": "node ./node_modules/@bentley/bentleyjs-tools/scripts/test-tsnode.js --testDir=./test/ --watch",
    "docs": "node ./node_modules/@bentley/bentleyjs-tools/scripts/docs.js --onlyJson --json=lib/docs/json/ec-js/file.json",
    "docs:dev": "npm run docs && bmsWatch --source ./lib/docs --destination public/",
    "coverage": "node scripts/cover.js",
    "start": "npm run build && npm run test && npm run docs"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@bentley/bemetalsmith": "^1.13.2-0",
    "@bentley/bentleyjs-tools": "^1.0.54",
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "^1.0.41"
  }
}
