{
  "name": "@bentley/imodeljs-core",
  "description": "iModel.js core components",
  "license": "UNLICENSED",
  "main": "./lib/main",
  "repository": {
    "type": "git",
    "url": "https://tfs.bentley.com/tfs/ProductLine/Platform%20Technology/_git/imodeljs-core"
  },
  "version": "1.1.4",
  "scripts": {
    "build": "gulp build",
    "build-release": "gulp build --release",
    "clean": "gulp clean",
    "lint": "node node_modules/@bentley/bentleyjs-tools/scripts/tslint.js",
    "test": "gulp test",
    "install": "node scripts/install-imodeljs-addon.js",
    "wip-install": "concurrently \"npm run wip-install-backend\" \"npm run wip-install-frontend\" \"npm run wip-install-test\"",
    "wip-install-backend": "cd source/backend && npm install",
    "wip-install-frontend": "cd source/frontend && npm install",
    "wip-install-test": "cd source/test && npm install",
    "wip-clean": "concurrently \"npm run wip-clean-backend\" \"npm run wip-clean-frontend\" \"npm run wip-clean-test\"",
    "wip-clean-backend": "cd source/backend && npm run clean",
    "wip-clean-frontend": "cd source/frontend && npm run clean",
    "wip-clean-test": "cd source/test && npm run clean",
    "wip-build": "concurrently \"npm run wip-build-backend\" \"npm run wip-build-frontend\" \"npm run wip-build-test\"",
    "wip-build-backend": "cd source/backend && npm run compile",
    "wip-build-frontend": "cd source/frontend && npm run compile",
    "wip-build-test": "cd source/test && npm run compile",
    "wip-test": "cd source/test && npm run test"
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "^1.0.35",
    "@bentley/geometry-core": "^1.0.11",
    "@bentley/imodeljs-clients": "^1.0.26",
    "js-base64": "^2.3.2"
  },
  "devDependencies": {
    "@bentley/bentleyjs-tools": "^1.0.49",
    "@types/chai": "^4.0.4",
    "@types/fs-extra": "^4.0.3",
    "@types/js-base64": "^2.3.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "commander": "^2.11.0",
    "concurrently": "^3.5.0",
    "debug-fabulous": "^0.1.2",
    "diff": "^3.3.1",
    "duplexify": "^3.5.1",
    "fs-extra": "latest",
    "gulp": "latest",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "inherits": "^2.0.3",
    "object-assign": "^4.1.1",
    "once": "^1.4.0",
    "os-locale": "^2.1.0",
    "resolve": "^1.5.0",
    "semver": "^5.4.1",
    "string-width": "^2.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
