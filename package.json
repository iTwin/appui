{
  "name": "@bentley/ecpresentation",
  "version": "0.0.1",
  "description": "EC presentation library",
  "keywords": [
    "Bentley",
    "EC",
    "Presentation",
    "iModelJS"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://tfs.bentley.com/tfs/ProductLine/Platform%20Technology/_git/ecpresentation"
  },
  "scripts": {
    "build": "echo use 'rush rebuild'",
    "clean": "concurrently \"npm run clean:backend\" \"npm run clean:frontend\" \"npm run clean:test\" \"npm run clean:sample\"",
    "clean:backend": "cd src/backend && npm run clean",
    "clean:frontend": "cd src/frontend && npm run clean",
    "clean:test": "cd src/test && npm run clean",
    "clean:sample": "cd src/sample && npm run clean",
    "publish": "concurrently \"npm run publish:backend\" \"npm run publish:frontend\"",
    "publish:backend": "cd src/backend && npm run build && npm publish",
    "publish:frontend": "cd src/frontend && npm run build && npm publish",
    "start": "cd src/sample && npm start",
    "lint": "node node_modules/@bentley/bentleyjs-tools/scripts/tslint.js",
    "test": "cd src/test && npm run test",
    "cover": "cd src/test && npm run cover"
  },
  "dependencies": {},
  "devDependencies": {
    "@bentley/bentleyjs-tools": "^1.0.58",
    "concurrently": "^3.5.0",
    "tslint": "^5.8.0",
    "tslint-consistent-codestyle": "^1.11.0",
    "typescript": "^2.6.2"
  }
}
