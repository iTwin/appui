{
  "name": "@bentley/ui-ninezone",
  "version": "1.11.0",
  "description": "iModel.js Nine-zone React UI components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/imodeljs/tree/master/ui/ninezone"
  },
  "main": "lib/ui-ninezone.js",
  "typings": "lib/ui-ninezone",
  "scripts": {
    "build": "buildIModelJsModule",
    "build:watch": "tsc -w",
    "clean": "rimraf lib out .rush demo/lib package-deps.json",
    "cover": "nyc npm test",
    "docs": "npm run docs:typedoc",
    "docs:typedoc": "node ./node_modules/@bentley/build-tools/scripts/docs.js --includes=../../generated-docs/extract --json=../../generated-docs/ui/ui-ninezone/file.json --tsIndexFile=./ui-ninezone.ts --onlyJson %TYPEDOC_THEME%",
    "docs:demo": "webpack --config ./demo/webpack.prod.ts",
    "extract-api": "node ./node_modules/@bentley/build-tools/scripts/extract-api.js --entry=ui-ninezone",
    "lint": "npm run tslint && npm run eslint",
    "eslint": "eslint src demo --ext .ts,.tsx,.js,.jsx",
    "tslint": "tslint --project . 1>&2 && tslint --project ./demo/tsconfig.json 1>&2",
    "pack": "npm run build && node ../../scripts/pack.js",
    "start": "webpack-dev-server --config ./demo/webpack.dev.ts",
    "test": "mocha --opts ../mocha.opts \"./lib/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts,tsx --watch"
  },
  "iModelJs": {
    "buildModule": {
      "type": "system",
      "sourceResources": [
        {
          "source": "./src/**/*.*css",
          "dest": "./lib"
        },
        {
          "source": "./public/**/*",
          "dest": "./lib/public"
        }
      ],
      "webpack": {
        "dest": "./lib/module",
        "entry": "./lib/ui-ninezone.js",
        "bundleName": "ui-ninezone",
        "styleSheets": true
      }
    }
  },
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "peerDependencies": {
    "@bentley/ui-core": "^1.11.0"
  },
  "//devDependencies": [
    "NOTE: All peerDependencies should also be listed as devDependencies since peerDependencies are not considered by npm install",
    "NOTE: All tools used by scripts in this package must be listed as devDependencies"
  ],
  "devDependencies": {
    "@bentley/build-tools": "1.11.0",
    "@bentley/icons-generic-webfont": "^0.0.43",
    "@bentley/ui-core": "1.11.0",
    "@bentley/webpack-tools": "1.11.0",
    "@testing-library/react": "^8.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7",
    "@types/chai-jest-snapshot": "^1.3.0",
    "@types/chai-spies": "^1.0.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "3.9.3",
    "@types/highlight.js": "^9.12.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.14.1",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-resize-detector": "^3.1.0",
    "@types/react-router-dom": "^4.2.7",
    "@types/sinon": "^7.5.1",
    "@types/testing-library__react-hooks": "^3.1.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-dev-server": "^3.1.6",
    "@typescript-eslint/parser": "^2.10.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7",
    "chai-jest-snapshot": "^2.0.0",
    "chai-spies": "1.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^6.7.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "highlight.js": "~9.14.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "3.0.2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "raf": "^3.4.0",
    "raf-schd": "^4.0.0",
    "raw-loader": "^0.5.1",
    "react-markdown": "^3.2.1",
    "react-resize-detector": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "svg-react-loader": "^0.4.5",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.3.2",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-etc": "^1.5.2",
    "tslint-loader": "^3.6.0",
    "tsutils": "^3.6.0",
    "typemoq": "^2.1.0",
    "typescript": "~3.7.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.1.4"
  },
  "//dependencies": [
    "NOTE: these dependencies should be only for things that DO NOT APPEAR IN THE API",
    "NOTE: imodeljs-frontend should remain UI technology agnostic, so no react/angular dependencies are allowed"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "nyc": {
    "extends": "./node_modules/@bentley/build-tools/.nycrc",
    "require": [
      "ignore-styles",
      "jsdom-global/register",
      "source-map-support/register",
      "ts-node/register"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
