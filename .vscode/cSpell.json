    "words": [
        "backend",
        "bentleyjs",
        "ecclass",
        "ecinstanceid",
        "ecsql",
        "endverbatim",
        "Guid",
        "imodel",
        "imodeljs",
        "mixins",
        "remotable",
        "remoting",
        "struct",
        "subtractive",
        "superclass",
        "superclasses",
        "underbar",
        "viewflags",
        "Wireframe"
    ],
    // flagWords - list of words to be always considered incorrect
